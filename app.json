{
  "expo": {
    "name": "Beauty-Blush",
    "slug": "Beauty-Blush",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./beautyblush/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./beautyblush/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./beautyblush/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.bossemmanuel.BeautyBlush"
    },
    "web": {
      "favicon": "./beautyblush/assets/favicon.png"
    },
    "plugins": [
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) needs access to your Camera."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "67e2ffda-0658-4dfb-abe9-bd798e3dacdc"
      }
    }
  }
}
